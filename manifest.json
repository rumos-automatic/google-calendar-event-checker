{
   "action": {
      "default_icon": {
         "128": "icons/icon128.png",
         "16": "icons/icon16.png",
         "48": "icons/icon48.png"
      },
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "sentry.7.100.1.bundle.min.js", "contentScript.js" ],
      "matches": [ "https://calendar.google.com/calendar/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Mark events as completed or cancelled in Google Calendar with emojis or strikethroughs.",
   "externally_connectable": {
      "matches": [ "https://*.calcheckr.com/*" ]
   },
   "homepage_url": "https://calcheckr.com",
   "host_permissions": [
      "https://calendar.google.com/*",
      "https://www.googleapis.com/*"
   ],
   "oauth2": {
      "client_id": "51083860430-s3d5b5s4gbbmibe5bnfe7asv82j0lpkb.apps.googleusercontent.com",
      "scopes": [
         "https://www.googleapis.com/auth/calendar.events"
      ]
   },
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Google Calendar Event Checker",
   "options_ui": {
      "open_in_tab": false,
      "page": "options.html"
   },
   "permissions": [ "storage", "identity" ],
   "short_name": "gcal-checker",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.1"
}
